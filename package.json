    "eslint": "^9.26.0",
    "globals": "^16.1.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "prettier": "3.5.3",
    "supertest": "^7.1.4"
  },
  "dependencies": {
    "aws-jwt-verify": "^5.1.0",
    "compression": "^1.8.0",
    "content-type": "^1.0.5",
